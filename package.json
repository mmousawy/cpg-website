    "@react-email/render": "2.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@vercel/analytics": "^1.6.1",
    "blurhash": "^2.0.5",

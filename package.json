    "@testing-library/react": "^16.3.2",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/sanitize-html": "^2.16.0",
    "@vitejs/plugin-react": "^5.1.2",
    "critters": "^0.0.25",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "supabase": "^2.74.4",

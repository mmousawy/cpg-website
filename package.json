    "image-size": "^2.0.2",
    "nanoid": "^5.1.6",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "photoswipe": "^5.4.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
